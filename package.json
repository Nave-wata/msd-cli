{
  "name": "cli-msd",
  "version": "1.0.0",
  "description": "CLI to migrate channel messages from Slack to Discord",
  "author": "@yogarasu",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "init": "node --loader ts-node/esm ./command/init.mts",
    "build": "run-s init \"build:*\"",
    "build:channel": "node --loader ts-node/esm ./command/build/channel.mts",
    "build:user": "node --loader ts-node/esm ./command/build/user.mts",
    "build:message": "node --loader ts-node/esm ./command/build/message.mts",
    "deploy": "run-p \"deploy:*\"",
    "deploy:channel": "node --loader ts-node/esm ./command/deploy/channel.mts",
    "deploy:message": "node --loader ts-node/esm ./command/deploy/message.mts",
    "destroy": "run-p \"deploy:*\"",
    "destroy:channel": "node --loader ts-node/esm ./command/destroy/channel.mts",
    "destroy:message": "node --loader ts-node/esm ./command/destroy/message.mts",
    "start": "run-s build deploy"
  },
  "volta": {
    "node": "18.7.0",
    "npm": "8.15.1"
  },
  "dependencies": {
    "@discordjs/rest": "^1.0.1",
    "@slack/web-api": "^6.7.2",
    "commander": "^9.4.0",
    "date-fns": "^2.29.1",
    "discord.js": "^14.2.0",
    "dotenv": "^16.0.1",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.5",
    "npm-check-updates": "^16.0.5",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
