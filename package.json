{
  "name": "msd-cli",
  "version": "1.0.0",
  "description": "CLI to migrate from Slack to Discord",
  "author": "@yogarasu",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "init": "node --loader ts-node/esm ./command/init.mts",
    "migrate": "run-s migrate:*",
    "migrate:channel": "node --loader ts-node/esm ./command/migrate/channel.mts",
    "migrate:user": "node --loader ts-node/esm ./command/migrate/user.mts",
    "migrate:message": "node --loader ts-node/esm ./command/migrate/message.mts",
    "build": "run-s init build:*",
    "build:channel": "node --loader ts-node/esm ./command/build/channel.mts",
    "build:user": "node --loader ts-node/esm ./command/build/user.mts",
    "build:message": "node --loader ts-node/esm ./command/build/message.mts",
    "deploy": "run-s deploy:*",
    "deploy:channel": "node --loader ts-node/esm ./command/deploy/channel2.mts",
    "deploy:user": "node --loader ts-node/esm ./command/deploy/user2.mts",
    "deploy:message": "node --loader ts-node/esm ./command/deploy/message2.mts",
    "destroy": "yes | run-s destroy:*",
    "destroy:user": "node --loader ts-node/esm ./command/destroy/user2.mts",
    "destroy:message": "node --loader ts-node/esm ./command/destroy/message2.mts",
    "destroy:channel": "node --loader ts-node/esm ./command/destroy/channel2.mts",
    "start": "run-s build deploy",
    "update": "npx ncu -u && npm install",
    "prisma": "yes | npx prisma migrate dev --name init && npx prisma migrate dev"
  },
  "volta": {
    "node": "18.7.0",
    "npm": "8.15.1"
  },
  "dependencies": {
    "@discordjs/rest": "^1.0.1",
    "@prisma/client": "^4.2.1",
    "@slack/web-api": "^6.7.2",
    "async-retry": "^1.3.3",
    "commander": "^9.4.0",
    "date-fns": "^2.29.2",
    "discord.js": "^14.2.0",
    "dotenv": "^16.0.1",
    "picocolors": "^1.0.0",
    "prisma": "^4.2.1",
    "prompts": "^2.4.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.5",
    "@types/node": "^18.7.8",
    "@types/prompts": "^2.0.14",
    "@types/uuid": "^8.3.4",
    "npm-check-updates": "^16.0.5",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
