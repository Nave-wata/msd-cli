generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model SlackChannel {
  id              Int              @id @default(autoincrement())
  name            String
  channelId       String           @unique
  type            Int
  topic           String
  isArchived      Boolean
  pins            String?
}
